{
 "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false
  },
  "files": [
    "src/app/**/*.ts"
  ],
  "exclude": [
    "typings",
    "node_modules"
  ],
  "types": [
      "core-js"
  ]
}